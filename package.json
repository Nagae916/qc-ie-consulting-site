{
  "name": "qc-ie-consulting-site",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "gen": "npx -y contentlayer2 build",
    "dev": "npm run gen && next dev -p 3000",
    "build": "npm run gen && next build",
    "start": "next start -p 3000",
    "lint:fast": "next lint --cache --max-warnings=0",
    "check:routes": "node ./scripts/check-routes.js",
    "check:dup": "node ./scripts/check-duplicate-urls.js",
    "check:audit": "node ./scripts/audit-guides.js",
    "check:fast": "npm run lint:fast && npm run check:routes && npm run check:dup && npm run check:audit",
    "check:ci": "npm run gen && npm run check:fast && next build",
    "lint": "next lint",
    "check": "npm run check:fast"
  },
  "dependencies": {
    "chart.js": "^4.4.3",
    "contentlayer2": "^0.5.0",
    "katex": "0.16.11",
    "next": "14.2.5",
    "next-contentlayer2": "^0.5.0",
    "react": "18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.3.1",
    "rehype-katex": "7.0.0",
    "remark-math": "6.0.0",
    "rss-parser": "3.12.0"
  },
  "devDependencies": {
    "@types/node": "20.14.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4"
  },
  "overrides": {
    "eslint": "8.57.0"
  },
  "engines": {
    "node": ">=18 <21"
  }
}
